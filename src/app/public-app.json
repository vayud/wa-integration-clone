{
  "name": "WhatsApp Integration (New)",
  "uid": "tf-whatsapp-integration-new",
  "description": "WhatsApp Business API Integration React",
  "logo": "./logo.png",
  "auth": {
    "redirectUrls": [
      "http://localhost:3000/oauth",
      "http://localhost/projects/whatsapp-integration/process.php",
      "https://whatsapp-integration.transfunnel.io/process.php"
    ],
    "requiredScopes": [
      "account-info.security.read",
      "automation",
      "crm.objects.contacts.read",
      "crm.objects.contacts.write",
      "crm.objects.owners.read",
      "crm.objects.users.read",
      "oauth",
      "tickets",
      "timeline"
    ],
    "optionalScopes": [
      "crm.schemas.contacts.read",
      "crm.schemas.contacts.write"
    ],
    "conditionallyRequiredScopes": [],
    "advancedScopeSettingsEnabled": true
  },
  "allowedUrls": [
    "https://whatsapp-integration.transfunnel.io/api/crm-card.php",
    "https://whatsapp-integration.transfunnel.io/api/contact-stats.php"
  ],
  "permittedUrls": {
    "fetch": [
      "https://whatsapp-integration.transfunnel.io/api/crm-card.php",
      "https://whatsapp-integration.transfunnel.io/api/contact-stats.php"
    ],
    "iframe": [
      "https://whatsapp-integration.transfunnel.io/tw/single-send.php",
      "https://whatsapp-integration.transfunnel.io/kl/single-send.php",
      "https://whatsapp-integration.transfunnel.io/chats.php",
      "https://whatsapp-integration.transfunnel.io/customer-portal.php"
    ],
    "img": [
      "https://whatsapp-integration.transfunnel.io/assets"
    ]
  },
  "extensions": {
    "crm": {
      "cards": [
        {
          "file": "./extensions/whatsapp-card.json"
        },
        {
          "file": "./extensions/preview-actions.json"
        },
        {
          "file": "./extensions/whatsapp-tab-card.json"
        },
        {
          "file": "./extensions/contact-stats.json"
        },
        {
          "file": "./extensions/contact-trends.json"
        },
        {
          "file": "./extensions/template-distribution.json"
        }
      ]
    }
  },
  "support": {
    "supportEmail": "productsupport@transfunnel.com",
    "documentationUrl": "https://www.transfunnel.com/marketplace/hubspot-whatsapp-integration/a-complete-guide"
  }
}